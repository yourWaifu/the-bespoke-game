material {
	name : bakedTexture,
	parameters : [
		{
			type : sampler2d,
			name : albedo
		}
	],
	requires : [
		uv0
	],
	shadingModel : lit,
	blending : masked,
	//doubleSided : true, //good for transparency
}

fragment {
	void material(inout MaterialInputs material) {
		prepareMaterial(material);
		material.baseColor.rgba = texture(materialParams_albedo, getUV0()).rgba;
	}
}
